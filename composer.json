{
  "name": "jl/onepage-nav",
  "type": "contao-bundle",
  "description": "This Onepage Bundle add a one page navigation module to Contao Open Source CMS. No jQuery required!",
  "keywords": ["onepage","one","page","navigation","article","contao"],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Jonas Linn"
    }
  ],
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Jl\\OnepageBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Jl\\OnepageNav\\ContaoManager\\Plugin"
  }
}
